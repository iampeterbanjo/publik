\n\n      // Parse URL for identifier\n      const identifier = url.split('?').shift().split('/').pop()\n\n      return identifier\n    } else {\n      return ''\n    }\n  }\n\n  static getProofStatement(searchText: string) {\n    const $ = cheerio.load(searchText)\n    const postContent = $('head > meta[property=\"og:title\"]')\n    let statement = ''\n\n    if (postContent !== undefined) {\n      statement = postContent.attr('content')\n    }\n\n    return statement\n  }\n}\n\nexport { LinkedIn }\n"},"lineCount":null}},"error":null,"hash":"f2f5931c905c889c501eac4eeb5e9a71","cacheData":{"env":{}}}